package Snippet;
import java.util.*;

public class PascalTriangle {
	
	public List<List<Integer>> generate(int numRows){
		if(numRows == 0){
			return null;
		}
		else{
			List<List<Integer>> l = new ArrayList<>();
			return generate(numRows, 1, l);
		}
	}
	
	private List<List<Integer>> generate(int numRows, int level, List<List<Integer>> temp){
		if(temp.size() == 0){
			ArrayList<Integer> row = new ArrayList<>();
			row.add(1);
			temp.add(row);
		}
		else{
			ArrayList<Integer> row = new ArrayList<>();
		}
	}
}
