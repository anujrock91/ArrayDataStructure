package Snippet;

public class FourSum {
	
	public List<List<Integer>> fourSum(int nums[], int target){
		
		for(int i=0;i<nums.length-3;i++){
			for(int j=i+1;j<nums.length-2;j++){
				int a = nums[i];
				int b = nums[j];
				int left = j+1;
				int right = (nums.length-1);
				while(left < right){
					int c = nums[left];
					int d = nums[right];
					int sum = (a+b+c+d);
					if(sum == target){
						
					}
					else if(sum < target){
						
					}
					else{
						
					}
				}
			}
		}
	}
}
