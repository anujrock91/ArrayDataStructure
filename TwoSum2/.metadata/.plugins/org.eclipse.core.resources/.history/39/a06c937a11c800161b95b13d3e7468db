package Snippet;

public class TwoSum {

	public int[] twoSum(int []nums, int target){
		int left = 0;
		int right = (nums.length-1);
		int result[] = new int[2];
		while(left<right){
			int sum = nums[left] + nums[right];
		}
		return result;
	}
	
	public static void main(String[] args) {
		
	}

}
