package Snippet;

public class BestTimeBuySell {
	
	public int maxProfit(int[] prices){
		int left = 0;
		int right = 0;
		int profit = 0;
		while(left< prices.length && right<prices.length){
			int currentDiff = (prices[left]-prices[right]);
			if(currentDiff > 0){
				
			}
			else{
				left = right;
			}
			++right;
		}
	}
}
