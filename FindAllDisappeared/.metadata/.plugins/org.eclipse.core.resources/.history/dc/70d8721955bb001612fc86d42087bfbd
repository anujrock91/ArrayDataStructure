	}
	
	public int [] getDisappeared(int [] arr){
		for(int i=0;i<arr.length;i++){
			if(arr[Math.abs(arr[i])-1] < 1){
				
			}
			arr[Math.abs(arr[i])-1] = (-1*arr[Math.abs(arr[i])-1]);
		}
	}

}
