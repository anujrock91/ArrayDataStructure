				continue;
			}
			arr[Math.abs(arr[i])-1] = (-1*arr[Math.abs(arr[i])-1]);
		}
		
		for(int i=0;i<arr.length;i++){
			if(arr[i] < 1){
				
			}
		}
	}

}
