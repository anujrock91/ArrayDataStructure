package Snippet;

public class RemoveDups {
	
	public static int[] removeDups(int [] arr){
		int left = 0;
		int right = 1;
		
		while(left< arr.length && right<arr.length){
			if(arr[left] != arr[right]){
				++left;
				arr[left] = arr[right];
			}
			++right;
		}
		for(int i=left+1;i)
		return 
	}
}
